<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="text_limit">
        <item name="android:lines">1</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="main_tab_me_text" parent="@style/text_limit">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/gray</item>
    </style>

    <style name="normal_field_name_text" parent="@style/text_limit">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="style_account_name" parent="@style/text_limit">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/acount_name</item>
    </style>

    <style name="account_key" parent="@style/text_limit">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/acount_name</item>
        <item name="android:gravity">left</item>
    </style>
    
    <style name="style_account_key" parent="@style/text_limit">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/acount_name</item>
        <item name="android:gravity">left</item>
    </style>

    <style name="account_value" parent="@style/text_limit">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">right</item>
    </style>

    <style name="about_value" parent="@style/text_limit">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">right</item>
    </style>

    <style name="address_item_address" parent="@style/text_limit">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">left</item>
    </style>

    <style name="title">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="me_city_item_title">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@color/background</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">10dp</item>
    </style>

    <style name="shared_title">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:maxLines">2</item>
    </style>

    <style name="shared_content">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/gray_word</item>
        <item name="android:maxLines">3</item>
    </style>

    <style name="contacts_index">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="layout_clickable">
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/layout_style_transparent_selector</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="custom_checkbox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_style_selector</item>
    </style>

    <style name="account_layout_clickable">
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/layout_style_transparent_selector</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="settings_layout_clickable">
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/layout_style_transparent_selector</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
    </style>

    <style name="settings_new_layout_clickable">
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/layout_style_white_selector</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
    </style>

    <style name="create_new_chat_layout_clickable">
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/layout_style_white_selector</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">15dp</item>
    </style>

    <style name="settings_grid_nickname">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/gray_dark</item>
    </style>

    <style name="style_title_btn_red_16">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:background">@drawable/btn_style_circle_red_selector</item>
    </style>

    <style name="style_title_btn_red_14">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:background">@drawable/btn_style_circle_red_selector</item>
    </style>

    <style name="style_title_btn_orange_16">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:background">@drawable/btn_style_circle_orange_selector</item>
    </style>

    <style name="style_title_btn_orange_14">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:background">@drawable/btn_style_circle_orange_selector</item>
    </style>

    <style name="btn_clickable_circle">
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/btn_style_circle_orange_selector</item>
    </style>

    <style name="btn_clickable_square">
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/btn_style_square_red_selector</item>
    </style>

    <style name="btn_unclickable_circle">
        <item name="android:enabled">false</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:background">@drawable/btn_style_circle_unclickable</item>
    </style>

    <style name="add_content_dialog" parent="android:Animation">
        <item name="@android:windowEnterAnimation">@anim/dialog_in</item>
        <item name="@android:windowExitAnimation">@anim/dialog_out</item>
        <item name="@android:windowFullscreen">true</item>
    </style>

    <style name="group_filter_dialog" parent="android:Animation">
        <item name="@android:windowEnterAnimation">@anim/slide_left_in</item>
        <item name="@android:windowExitAnimation">@anim/slide_right_out</item>
        <item name="@android:windowFullscreen">true</item>
    </style>

    <style name="custom_progress_dialog" parent="@android:style/Theme.Light.Panel">
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
    </style>
    
    <style name="lucky_money_dialog" parent="@android:style/Theme.Light.Panel">
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="chat_group_description">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="chat_settings_view" parent="@style/text_limit">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/acount_name</item>
        <item name="android:gravity">left</item>
    </style>

    <style name="post_detail_date_view" parent="@style/text_limit">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/gray_dark</item>
        <item name="android:text">11-11-34 12:34</item>
    </style>

    <style name="chat_item_share_image_view">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">60dp</item>
    </style>

    <style name="chat_msg_send_status">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>

    <style name="chat_msg_and_head_icon_margin_left">
        <item name="android:layout_marginLeft">5dp</item>
    </style>

    <style name="chat_msg_and_head_icon_margin_right">
        <item name="android:layout_marginRight">5dp</item>
    </style>

    <style name="chat_item_head_margin_right">
        <item name="android:layout_marginRight">10dp</item>
    </style>

    <style name="chat_item_head_margin_left">
        <item name="android:layout_marginLeft">10dp</item>
    </style>

    <style name="chat_item_share_minwidth">
        <item name="android:minWidth">60dp</item>
    </style>
    
    <style name="chat_msg_lucky_money_des_txt">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">15sp</item>
        <item name="android:gravity">left|top</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_marginBottom">3dp</item>
    </style>
    
    <style name="chat_msg_lucky_money_des_associate_txt">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">13sp</item>
        <item name="android:gravity">left|top</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_marginTop">2dp</item>
    </style>
    
    <style name="chat_msg_lucky_money_type_txt">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/orange_yellow_fade</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">left</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_marginTop">2dp</item>
    </style>

    <style name="float_view_anim">
        <item name="@android:windowEnterAnimation">@anim/float_window_slide_in</item>
        <item name="@android:windowExitAnimation">@anim/float_window_slide_out</item>
    </style>

    <style name="home_meeting_quantity">
        <item name="android:textColor">@color/gray_dark</item>
        <item name="android:textSize">12sp</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingRight">2dp</item>
        <item name="android:background">@drawable/home_meeting_quantity_style</item>
    </style>
</resources>