<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.handle.law"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".LawApplication"
        android:exported="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".activity.mine.AccountManageActivity"/>
        <activity android:name=".activity.mine.AddAddressActivity"/>
        <activity android:name=".activity.mine.FeedBackActivity"/>
        <activity android:name=".activity.mine.PrivateInfoActivity"/>
        <activity android:name=".activity.mine.SetMessageNotifyActivity"/>
        <activity android:name=".activity.mine.SettingActivity"/>
        <activity android:name=".activity.friend.FriendSearchActivity"/>
    </application>

</manifest>
